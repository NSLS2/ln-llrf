record(calcout, "$(P)Drv:PhaMeanWrap-Calc_") {
  # Unfortunately the modulo operator '%' casts to integer first
  field(CALC, "A>180?A-360:(A<-180?A+360:A)")
  field(OUT , "$(P)Drv:PhaMean-I PP MSS")
}

